<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android" >
    <CheckBoxPreference 
        	android:key="pref_enabled" 
        	android:title="@string/pref_enabled" 
        	android:defaultValue="true" android:summary="@string/pref_enabled_descr"/>
    <EditTextPreference android:key="pref_userkey" 
        	android:title="@string/pref_userkey" android:summary="@string/pref_userkey_descr"/>
    <EditTextPreference  
        	android:title="@string/pref_refresh_timeout" 
        	android:key="pref_refresh_timeout"
        	android:inputType="number"
        	android:defaultValue="5"/>    
</PreferenceScreen>
